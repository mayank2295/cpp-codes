// deletion in cpp 
# include <iostream> 
using namespace std; 
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i =0;i<n;i++){
        cin>>arr[i];
    }
    
    // to delete any random index value in an array 
    int index;
    cin>>index;
    for(int i = index;i<n;i++){
        arr[i] = arr[i+1];
    }
    n--;
    for(int i =0;i<n;i++){
        cout<<arr[i]<<" ";
    }
}
